// Se redefine un método en la clase hija y se muestra que se accede a la nueva versión de él.
// Resultado esperado: Redefinido.

class Clase1{

	var int a1;

	dynamic void setA1(int newValue){
		a1 = newValue;
		(System.printIln(a1));
	}	
}

class Clase2 extends Clase1{

	dynamic void setA1(int newValue){
		(System.printSln("Redefinido"));
	}	

	static void main()
	var Clase2 obj;	
	{		 
	  obj = new Clase2();
	  (obj.setA1(4));	  
	}
	
}