// Dos clases que usan nombres de variables y de métodos idénticos, para mostrar que no hay conflicto.

class Clase1{

	var int a1;

	dynamic void setA1(int newValue){
		a1 = newValue;
		(System.printI(a1));
	}

	static void main()
	var Clase2 obj;	
	{		 
	  obj = new Clase2();
	  (obj.setA1(4));	  
	}
}

class Clase2{

	var int a1;

	dynamic void setA1(int newValue){
		a1 = newValue;
		(System.printI(a1));
	}	
}