// Tests de operadores ==, != con tipos primitivos.

class Test{

	static void main()
	var boolean res;
	{		
		(System.printSln("1 == 1"));
		res = 1 == 1;	
		(System.printBln(res));
		
		(System.printSln("1 == 2"));
		res = 1 == 2;	
		(System.printBln(res));
		
		(System.printSln("1 != 1"));
		res = 1 != 1;	
		(System.printBln(res));
		
		(System.printSln("1 != 2"));
		res = 1 != 2;	
		(System.printBln(res));
		
		(System.printSln("'a' == 'a'"));
		res = 'a' == 'a';
		(System.printBln(res));
		
		(System.printSln("'a' != 'a'"));
		res = 'a' != 'a';
		(System.printBln(res));
	}
}